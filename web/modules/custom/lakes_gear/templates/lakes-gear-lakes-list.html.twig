</div>
<div class="lake-list">
  {% for lake in lakes %}
    <div class="lake-list-item">
      <a href="{{ path('lakes_gear.store.lake', {'lake_id': lake.lake_id}) }}">
        <img src="{{ lake.image_url }}" alt="{{ lake.name }}" />
